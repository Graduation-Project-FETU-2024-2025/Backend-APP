  "components": {
    "schemas": {
      "BranchDTO": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid",
            "nullable": true
          },
          "pharmacyId": {
            "type": "string",
            "format": "uuid"
          },
          "aR_BranchName": {
            "type": "string",
            "nullable": true
          },
          "eN_BranchName": {
            "type": "string",
            "nullable": true
          },
          "deliveryRange": {
            "type": "integer",
            "format": "int32"
          },
          "pricePerKilo": {
            "type": "number",
            "format": "double"
          },
          "minDeliveryPrice": {
            "type": "number",
            "format": "double"
          },
          "status": {
            "type": "string",
            "nullable": true
          },
          "image": {
            "type": "string",
            "nullable": true
          },
          "phoneNumber": {
            "type": "string",
            "nullable": true
          },
          "lat": {
            "type": "number",
            "format": "double"
          },
          "long": {
            "type": "number",
            "format": "double"
          },
          "workingHours": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/WorkingPeriodDTO"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "LoginDTO": {
        "required": [
          "email"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          }
        },
        "additionalProperties": false
      },
      "RegisterationDTO": {
        "required": [
          "confirmPassword",
          "email",
          "password",
          "pharmacyId",
          "userName"
        ],
        "type": "object",
        "properties": {
          "pharmacyId": {
            "type": "string",
            "format": "uuid"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "userName": {
            "maxLength": 100,
            "minLength": 3,
            "type": "string"
          },
          "password": {
            "minLength": 1,
            "type": "string",
            "format": "password"
          },
          "confirmPassword": {
            "minLength": 1,
            "type": "string"
          },
          "image": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "TestOtpDTO": {
        "required": [
          "email",
          "otp"
        ],
        "type": "object",
        "properties": {
          "otp": {
            "maxLength": 4,
            "minLength": 4,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          }
        },
        "additionalProperties": false
      },
      "WorkingPeriodDTO": {
        "type": "object",
        "properties": {
          "start": {
            "type": "string",
            "nullable": true
          },
          "end": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
